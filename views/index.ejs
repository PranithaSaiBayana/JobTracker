	function jobdisplay(i) {
		$("#updatehide").hide();
		var url = "http://localhost:2000/joblisting/" + i;

		$.ajax({
			url: url,
			type: "GET",
			success: function (result) {


				$("#jdiddisplayhidden").text(result[0][0].jobdescriptionId);
				$("#reqiddisplay").text(result[0][0].requirmentId);
				$("#jlidedit").val(result[0][0].joblocation);
				$("#expedit").val(result[0][0].experience);
				$("#lfedit").val(result[0][0].locationflexibility);
				$("#jdedit").val(result[0][0].jobdescription);
				$("buedit").val(result[0][0].bussinessunit).attr('selected', 'selected');
				$("#sregionedit").val(result[0][0].salesregions).attr('selected', 'selected');
				$("#srepedit").val(result[0][0].salesrep).attr('selected', 'selected');
				$("#statuspedit").val(result[0][0].status).attr('selected', 'selected');;
				$("#psedit").val(result[0][0].primaryskills);
				$("#ssedit").val(result[0][0].secondaryskills);
				$("#cbyedit").val(result[0][0].createdby);
				$("#ubyedit").val(result[0][0].updateby);
				$("#nposedit").val(result[0][0].noofpositions);
				$("#roleedit").val(result[0][0].role);
				$("#cbydateedit").val(result[0][0].createddate.slice(0, 19));
				$("#ubydateedit").val(result[0][0].updatedtime.slice(0, 19));
				$("#datereqedit").val(result[0][0].dateofreq.slice(0, 19));
				$("#expdateedit").val(result[0][0].expecteddate.slice(0, 19));
				$("#custedit").val(result[0][0].customers).attr('selected', 'selected');
				$("#JD_comment").val(result[0][0].jdcomments);
				$("#BU_comment").val(result[0][0].bucomments);
			}
		});
	}



