	function addjd() {
		
		var Requirement = $('[name="Requirementid"]').val();
		var Role = $('[name="role"]').val();
		var Position = $('[name="positions"]').val();
		var Expected = $('[name="ex_date"]').val();
		//var dateofreq = $('[name="sregion_edit"]').val();
		var Exp = $('[name="experience"]').val();
		var PSkill = $('[name="primary_skill"]').val();
		var SSkill = $('[name="secondry_skill"]').val();
		var locationflex = $('[name="loc_flex"]').val();
		var customerid = $('[name="customer"]').val();
		var Status = $('[name="status"]').val();
		var buid = $('[name="bu_unit"]').val();
		var salesregionid = $('[name="sales_region"]').val();
		var salesrepid = $('[name="sale_rep"]').val();
		var joblocation = $('[name="job_location"]').val();
		var jDescription = $('[name="job_description"]').val();
		var buComments = $('[name="buComments"]').val();
		var jdComments = $('[name="jd_comments"]').val();
		var createdby = 'Admin';
		// var createdate = $('[name="cust_edit"]').val();
		var updateby = 'Admin';
		//  var updatetime = $('[name="bucomments"]').val();



		var data = {
			"Requirement": Requirement,
			"Role": Role,
			"Position": Position,
			"Expected": Expected,
			"Exp": Exp,
			"PSkill": PSkill,
			"SSkill": SSkill,
			"locationflex": locationflex,
			"customerid": customerid,
			"Status": Status,
			"buid": buid,
			"salesregionid": salesregionid,
			"salesrepid": salesrepid,
			"joblocation": joblocation,
			"jDescription": jDescription,
			"buComments": buComments,
			"jdComments": jdComments,
			"createdby": createdby,
			"updateby": updateby

		}

		data = JSON.stringify(data);
		var url = "http://localhost:2000/jdescription";
		console.log(url);
		if(Requirement!="" && Role!="" && Position!=""&& Expected!=""&& Exp!=""&& PSkill!=""&& SSkill!=""&& locationflex!=""&& customerid!=""
		&& Status!=""&& buid!=""&& salesregionid!=""&& joblocation!=""&& jDescription!="")
		  {
		$.ajax({
			url: url,
			type: "POST",
			async: false,
			data: data,
			contentType: "application/json",
			dataType: "json",
			success: function (result) {

				$('#successmodal').modal('show');
				

			},
			error: function () {
				
			}
		});

	}
	}


